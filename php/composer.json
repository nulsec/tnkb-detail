{
  "name": "nulsec/tnkb-client-php",
  "description": "Indonesian Vehicle Registration Number (TNKB) Decoder - PHP Client",
  "type": "library",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Security Research Team",
      "email": "security@nulsec.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "TNKB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TNKB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "lint": "phpstan analyse src",
    "format": "php-cs-fixer fix src --allow-risky=yes",
    "format-check": "php-cs-fixer fix src --dry-run --allow-risky=yes"
  }
}
